AA,IAAA;AAEA,WAAA,SAAA;AAEA,KAVA;AAYA,IAAA,SAZA,uBAYA;AAAA;;AACA,WAAA,WAAA,CAAA,+BAAA,EACA,IADA,CACA,YAAA;AAAA;AACA,QAAA,MAAA,CAAA,KAAA,IAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA,gBADA;AAEA,UAAA,QAAA,EAAA,uBAFA;AAGA,UAAA,QAAA,EAAA,WAHA;AAKA,UAAA,EAAA,EAAA,4BALA;AAMA,UAAA,IAAA,EAAA,EANA;AAOA,UAAA,OAAA,EAAA,oCAAA,KAAA,CAAA,IAAA,CAAA,IAPA;AAQA,UAAA,IAAA,qMAIA,KAAA,CAAA,IAAA,CAAA,IAJA,0JAQA,KAAA,CAAA,IAAA,CAAA,QARA,cAQA,KAAA,CAAA,IAAA,CAAA,OARA,uJAYA,KAAA,CAAA,IAAA,CAAA,KAZA,wJAgBA,KAAA,CAAA,IAAA,CAAA,SAhBA;AARA,SAAA,EA6BA,IA7BA,CA6BA,YAAA;AACA,UAAA,KAAA,CAAA,MAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,IAAA;;AACA,UAAA,KAAA,CAAA,KAAA;AACA,SAjCA,CAAA;AAmCA,OArCA,EAsCA,KAtCA,CAsCA,UAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,CAAA,CAAA;AACA,QAAA,KAAA,CAAA,MAAA,GAAA,KAAA,CAFA,CAGA;AACA,OA1CA;AA2CA,KAxDA;AA4DA,IAAA,KA5DA,mBA4DA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA,KA9DA;AAgEA,IAAA,eAhEA,6BAgEA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,eAAA;AACA;AAlEA;AAzBA,CAAA","sourcesContent":["\n  <template>\n  <v-container class=\"grey lighten-5\">\n    <h1\n      class=\"transparent--text pt-3\"\n      id=\"Contacto\"\n    >CONTACTO</h1>\n    <br>\n    <br>\n    <v-row no-gutters>\n      <v-col>\n\n        <v-card-text class=\"black--text pt-4\">\n\n        </v-card-text>\n        <v-container>\n          <v-img\n            :src=\"`data:image/png;base64,`\"\n            height=\"200\"\n            max-width=\"350\"\n            contain\n            class=\"pm-6 mx-auto\"\n          />\n        </v-container>\n\n      </v-col>\n      <v-col cols=\"1\">\n\n      </v-col>\n      <v-col>\n        <v-toolbar\n          color=\"blue darken-3\"\n          cards\n          dark\n          flat\n        >\n          <v-icon style=\"#007ebf;  font-size:40px;\">mdi-at</v-icon>\n          <v-card-title\n            class=\"font-weight-black\"\n            style=\"min-width:300px\"\n          >\n            Contáctenos\n\n          </v-card-title>\n          <v-spacer></v-spacer>\n\n        </v-toolbar>\n\n        <v-card\n          class=\"pa-6\"\n          outlined\n          tile\n          flat\n        >\n          <v-form\n            ref=\"form\"\n            v-model=\"valid\"\n            lazy-validation\n          >\n\n            <v-text-field\n              v-model=\"form['name']\"\n              dense\n              :rules=\"nameRules\"\n              label=\"Apellido y Nombre\"\n              clearable\n              required\n              class=\"pr-10 pl-10\"\n            ></v-text-field>\n\n            <v-row>\n              <v-col\n                cols=\"12\"\n                sm=\"6\"\n              >\n                <v-text-field\n                  v-model=\"form['tel_area']\"\n                  label=\"Cod.Area\"\n                  type=\"number\"\n                  dense\n                  prefix=\"+54 - 0\"\n                  clearable\n                  required\n                  class=\"pr-10 pl-10\"\n                ></v-text-field>\n              </v-col>\n              <v-col\n                cols=\"12\"\n                sm=\"6\"\n              >\n                <v-text-field\n                  v-model=\"form['tel_nro']\"\n                  type=\"number\"\n                  dense\n                  label=\"Nro.Celular\"\n                  prefix=\"15\"\n                  clearable\n                  required\n                  class=\"pr-10 pl-10\"\n                ></v-text-field>\n              </v-col>\n            </v-row>\n\n            <v-text-field\n              v-model=\"form['email']\"\n              dense\n              :rules=\"emailRules\"\n              label=\"E-mail\"\n              clearable\n              required\n              class=\"pr-10 pl-10\"\n            ></v-text-field>\n\n            <v-textarea\n              v-model=\"form.desMotivo\"\n              color=\"teal\"\n              class=\"pr-10 pl-10\"\n            >\n              <template v-slot:label>\n                <div>\n                  Describinos tu consulta <small>(opcional)</small>\n                </div>\n              </template>\n            </v-textarea>\n            <v-row\n              align=\"center\"\n              justify=\"center\"\n              mb-5\n              style=\"margin-bottom:20px\"\n            >\n              <v-btn\n                :disabled=\"!valid\"\n                :small=\"$vuetify.breakpoint.mobile\"\n                color=\"info\"\n                @click=\"submitForm\"\n                rounded\n              >\n                Consultar\n              </v-btn>\n\n              <v-btn\n                color=\"info\"\n                :small=\"$vuetify.breakpoint.mobile\"\n                @click=\"reset\"\n                rounded\n              >\n                Cancelar\n              </v-btn>\n            </v-row>\n\n          </v-form>\n          <v-dialog\n            v-model=\"dialog\"\n            hide-overlay\n            persistent\n            width=\"300\"\n          >\n            <v-card\n              color=\"primary\"\n              dark\n            >\n              <v-card-text>\n                Enviando...\n                <v-progress-linear\n                  indeterminate\n                  color=\"white\"\n                  class=\"mb-0\"\n                ></v-progress-linear>\n              </v-card-text>\n            </v-card>\n          </v-dialog>\n\n          <v-dialog\n            v-model=\"conf\"\n            hide-overlay\n            persistent\n            width=\"300\"\n          >\n            <v-card\n              color=\"#007ebf\"\n              dark\n            >\n              <v-card-title>\n                Mensaje enviado con exito!\n\n              </v-card-title>\n              <v-card-text>\n                Muchas gracias por su consulta, en breve nos comunicaremos con usted.\n\n              </v-card-text>\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn\n                  color=\"white\"\n                  text\n                  @click=\"conf = false\"\n                >\n                  Cerrar\n                </v-btn>\n\n              </v-card-actions>\n            </v-card>\n          </v-dialog>\n        </v-card>\n\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n\n<script>\nexport default {\n  name: 'mapaContacto',\n  data () {\n\n    return {\n      form: {\n        name: '',\n        tel_area: '',\n        tel_nro: '',\n        email: '',\n        desMotivo: '',\n      },\n      dialog: false,\n      conf: false,\n      valid: true,\n      emailRules: [\n        v => !!v || 'E-mail es requerido',\n        v => /.+@.+\\..+/.test(v) || 'E-mail debe ser válido',\n      ],\n      nameRules: [\n        v => !!v || 'Nombre es requerido',\n        v => (v && v.length >= 10) || 'La longitud del nombre debe ser mayor a 10 caracteres',\n      ],\n    }\n  },\n  methods: {\n\n    submitForm () {\n      if (!this.$refs.form.validate()) {\n        return\n      }\n      this.dialog = true\n\n      this.sendEmail()\n\n    },\n\n    sendEmail () {\n      this.$loadScript(\"https://smtpjs.com/v3/smtp.js\")\n        .then(() => {                                         //this Promise return nothing!!!\n          window.Email && window.Email.send({\n            Host: \"smtp.gmail.com\",\n            Username: \"gaston.evsa@gmail.com\",\n            Password: \"g32488379\",\n\n            To: 'gastonviotti1987@gmail.com',\n            From: \"\",\n            Subject: \"Solicitud de Contacto WEB EVSA:\" + this.form.name,\n            Body: `<html>\n                   <table style=\"border: 1px solid black\">\n                   <tr>\n                   <td><strong>Apellido y Nombre:</strong></td>\n                   <td>\"${this.form.name}\"</td>\n                   </tr>\n                   <tr>\n                   <td><strong>Telefono:</strong></td>\n                   <td>\"${this.form.tel_area}-${this.form.tel_nro}\"</td>\n                   </tr>\n                   <tr>\n                   <td><strong>Email:</strong></td>\n                   <td>\"${this.form.email}\"</td>\n                   </tr>\n                   <tr>\n                   <td><strong>Motivo:</strong></td>\n                   <td>\"${this.form.desMotivo}\"</td>\n                   </tr>\n                   </table>\n                   </html>\n                   `\n          }).then(() => {\n            this.dialog = false\n            this.conf = true\n            this.reset()\n          }\n          );\n        })\n        .catch((e) => {\n          alert(e)\n          this.dialog = false\n          // Failed to fetch script\n        })\n    },\n\n\n\n    reset () {\n      this.$refs.form.reset()\n    },\n\n    resetValidation () {\n      this.$refs.form.resetValidation()\n    },\n  },\n}\n\n\n</script>\n"],"sourceRoot":"src/components"}]}